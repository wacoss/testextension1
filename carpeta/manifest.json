{
   "background": {
      "scripts": [ "background.js", "event.js", "global.js" ]
   },
   "browser_action": {
      "default_icon": "img/icon128.png",
      "default_popup": "m3u8.html"
   },
   "content_scripts": [ {
      "css": [ "css/materialdesignicons.min.css" ],
      "js": [ "script_in_content.js", "m3u8.js", "background.js" ],
      "matches": [ "*://*/*" ],
      "run_at": "document_start"
   } ],
   "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
   "description": "It opens a new tab on a video player",
   "icons": {
      "128": "img/icon128.png",
      "32": "img/icon32.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "M3U8 finder and HLS player",
   "offline_enabled": true,
   "omnibox": {
      "keyword": "m3u8"
   },
   "options_ui": {
      "chrome_style": true,
      "page": "m3u8_options.html"
   },
   "permissions": [ "activeTab", "\u003Call_urls>", "webRequest", "webRequestBlocking", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.1",
   "web_accessible_resources": [ "*.js", "*.html" ]
}
